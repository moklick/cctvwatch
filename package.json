{
  "name": "cctvwatch",
  "version": "0.0.0",
  "devDependencies": {
    "gulp": "3.5.2",
    "gulp-load-plugins": "~0.3.0",
    "gulp-browserify": "~0.4.4",
    "gulp-util": "~2.2.14",
    "gulp-autoprefixer": "~0.0.6",
    "gulp-csso": "~0.2.3",
    "gulp-jshint": "~1.4.0",
    "gulp-imagemin": "~0.1.5",
    "gulp-clean": "~0.2.4",
    "gulp-uglify": "~0.2.1",
    "gulp-concat": "~2.1.7",
    "gulp-cache": "~0.1.1",
    "gulp-less": "~1.2.1",
    "gulp-size": "~0.1.3",
    "gulp-connect": "~0.3.1",
    "proxy-middleware": "^0.5.0",
    "connect-memorize": "^0.1.2",
    "stringify": "^2.1.0",
    "browserify": "~3.29.0",
    "debowerify": "~0.5.2",
    "mocha": "~1.17.1",
    "should": "~3.1.2",
    "jshint-stylish": "~0.1.5",
    "browserify-shim": "~3.2.2",
    "gulp-if": "^1.2.0",
    "rsyncwrapper": "^0.3.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "lodash": "./app/bower_components/lodash/dist/lodash.js"
  },
  "browserify-shim": {
    "lodash": "global:lodash"
  },
  "dependencies": {
    "backbone": "~1.1.2",
    "jquery": "~2.1.0",
    "leaflet": "^0.7.3"
  },
  "scripts": {
    "postinstall": "bower install && cd app/bower_components/leaflet-bower/ && npm install"
  },
  "devConfig": {
    "deployURI": "cctv@cctv.antares.uberspace.de:/home/cctv/html",
    "proxy": {
      "enabled": true,
      "backendURL": "http://cctv.antares.uberspace.de/",
      "routes": [
        "api/v1/cctv",
        "auth/twitter",
        "auth/google",
        "auth/github",
        "auth/logout",
        "auth/token"
      ],
      "cacheDir": "cache"
    }
  }
}
